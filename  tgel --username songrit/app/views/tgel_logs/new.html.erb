<h1>New tgel_log</h1>

<% form_for(@tgel_log) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :log_type %><br />
    <%= f.text_field :log_type %>
  </p>
  <p>
    <%= f.label :message %><br />
    <%= f.text_field :message %>
  </p>
  <p>
    <%= f.label :controller %><br />
    <%= f.text_field :controller %>
  </p>
  <p>
    <%= f.label :action %><br />
    <%= f.text_field :action %>
  </p>
  <p>
    <%= f.label :params %><br />
    <%= f.text_area :params %>
  </p>
  <p>
    <%= f.label :session %><br />
    <%= f.text_area :session %>
  </p>
  <p>
    <%= f.label :tgel_user_id %><br />
    <%= f.text_field :user_id %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', tgel_logs_path %>